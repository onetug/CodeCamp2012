@model OCC.UI.Webhost.Models.HomeModel

@{
    ViewBag.Title = "Home Page";
}

<div>

@foreach (var announcement in ViewData.Model.Announcements)
{
    <div>
        <div><h3>@announcement.Title</h3></div>
        <div><i>@announcement.Subtitle</i></div>
        <div>@announcement.Content</div>
    </div>
}

</div>