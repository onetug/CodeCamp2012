<!DOCTYPE html>
<html>
    <head>
        <title>@ViewBag.Title</title>
        <link href="@Url.Content("~/Content/themes/Shared/onetug.113x132.jpg")" rel="shortcut icon"/>
        <link href="@Url.Content("~/Content/css/Layout.css")" rel="stylesheet" type="text/css" />
        <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"> </script>
        <link href="@Url.Content("~/Content/css/styles.css")" rel="stylesheet" type="text/css" />
    </head>
    <body>
        <div class="container_12 siteContainer">
            <div id="header" class="grid_8">
                <a href="@Url.Action("Index", "Home")"></a>
                <h1>Orlando Code Camp 2012</h1>
                <img class="onetugTree" src="../../Content/themes/Metro/Images/OnetugTree.White.43x51.AlphaBackground.png" alt="@@ONETUG on Twitter"/>
                <span id="dateAndVenue" nowrap>March 31, 2012 - Seminole State College</span>
            </div>
            <div id="tilesColumnRight" >
            @if (IsSectionDefined("TilesOnRightSide"))
            {
                @RenderSection("TilesOnRightSide")
            }
            else
            {
                <div id="tilesColumnRight">
                    @Html.Action("SingleMetroTileForSponsors","MetroTile")
                    @Html.Action("SingleMetroTileForAccount", "MetroTile")
                    @Html.Action("SingleMetroTileForAgenda", "MetroTile")
                    @Html.Action("SingleMetroTileForVenue", "MetroTile")
                    @Html.Action("DoubleMetroTileForTwitter", "MetroTile")
                    @Html.Action("SingleMetroTileForSpeakers", "MetroTile")
                    @Html.Action("SingleMetroTileForVolunteers", "MetroTile")
                </div>
            }
            </div>
            <div class="grid_8" >
                @RenderBody()
            </div>
            <div>
            </div>
        </div>
    </body>
</html>