@using OCC.UI.Webhost.Models
@model SessionViewModel

@{
    ViewBag.Title = "Sessions";
}

<div>
    @foreach (var track in ViewData.Model.Tracks)
    {
        <div>
            <h3>@track.Name</h3>
            @foreach (var sessions in track.Sessions)
            {
                <div>
                    <h4>@sessions.Name</h4>
                </div>
            }
        </div>
    }
</div>